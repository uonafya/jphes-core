<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
  "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
  "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

  <include file="dhis-web-commons.xml" />

  <package name="jphes-web-hierarchy" extends="dhis-web-commons" namespace="/jphes-web-hierarchy">

    <!-- JPHES Web APP -->

    <action name="index" class="org.hisp.dhis.hierarchy.action.NoAction">
      <result name="success" type="velocity">/main.vm</result>
      <param name="page">/jphes-web-hierarchy/index.vm</param>
      <param name="menu">/jphes-web-hierarchy/menu.vm</param>
    </action>

    <action name="allPartner" class="org.hisp.dhis.hierarchy.action.GetPartnerListAction">
      <result name="success" type="velocity">/main.vm</result>
      <param name="page">/jphes-web-hierarchy/allPartner.vm</param>
      <param name="menu">/jphes-web-hierarchy/menu.vm</param>
      <param name="javascripts">javascript/partner.js,javascript/filterTable.js</param>
      <!--<param name="requiredAuthorities">F_JPHES_LIST_PARTNER</param>-->
    </action>

    <action name="showAddPartnerForm" class="org.hisp.dhis.hierarchy.action.NoAction">
      <result name="success" type="velocity">/main.vm</result>
      <param name="page">/jphes-web-hierarchy/addPartnerForm.vm</param>
    </action>

    <action name="addPartner" class="org.hisp.dhis.hierarchy.action.AddPartnerAction">
      <result name="success" type="redirect">allPartner.action?currentPage=${keyCurrentPage}&amp;key=${keyCurrentKey}</result>
      <result name="error" type="redirect">showAddPartnerForm.action</result>
    </action>


  </package>
</struts>
