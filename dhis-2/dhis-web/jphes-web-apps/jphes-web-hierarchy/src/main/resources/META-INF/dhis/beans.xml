<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

  <bean id="NoAction" class="org.hisp.dhis.jphes.hierarchy.action.NoAction" scope="prototype" />

  <bean id="GetPartnerListAction" class="org.hisp.dhis.jphes.hierarchy.action.GetPartnerListAction" scope="prototype" >
    <property name="partnerService" ref="org.hisp.dhis.jphes.hierarchy.HierarchyService" />
    <property name="userService" ref="org.hisp.dhis.user.UserService" />
  </bean>

  <bean id="AddPartnerAction" class="org.hisp.dhis.jphes.hierarchy.action.AddPartnerAction" scope="prototype" >
    <property name="partnerService" ref="org.hisp.dhis.jphes.hierarchy.HierarchyService" />
    <property name="userService" ref="org.hisp.dhis.user.UserService" />
  </bean>

</beans>
